    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DataServer -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DataServer">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/ICTDevice"/>
        <dc:description>In computing, a data server is a piece of computer hardware or software (computer program) that provides functionality for other programs or devices, called &quot;clients&quot;.</dc:description>
        <rdfs:comment>In this ontology we focus on the hardware of data servers.</rdfs:comment>
        <rdfs:label>Data Server</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DataTransportRate -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DataTransportRate">
