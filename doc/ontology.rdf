    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Fan -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Fan">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/CoolingSystem"/>
