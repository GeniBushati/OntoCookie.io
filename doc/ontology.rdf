        <rdfs:label xml:lang="en">Device Owner</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DeviceSpeed -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DeviceSpeed">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/FunctionalIndicator"/>
