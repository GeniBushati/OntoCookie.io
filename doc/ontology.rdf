        <rdfs:label>Antivirus Software</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Apple -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Apple">
