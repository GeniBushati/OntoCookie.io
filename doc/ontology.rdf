        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/HardwareComponent"/>
        <dc:description>A computer case, also known as a computer chassis, is the enclosure that contains most of the components of a personal computer.</dc:description>
        <rdfs:label>Chassis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Chipset -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Chipset">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/HardwareComponent"/>
        <dc:description>A chipset is a set of electronic components in one or more integrated circuits known as a &quot;Data Flow Management System&quot; that manages the data flow between the processor, memory and peripherals. It is usually found on the motherboard.</dc:description>
        <rdfs:label>Chipset</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/CircularStrategy -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/CircularStrategy">
        <dc:description xml:lang="en">A range of efficiency and productivity enhancing activities carried out within the Circular Economy (CE).</dc:description>
        <rdfs:label xml:lang="en">Circular Strategy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Client -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Client">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Role"/>
        <dc:description xml:lang="en">An agent seeking a service for his/her device.</dc:description>
        <rdfs:label>Client</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Cobalt -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Cobalt">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/NonFerrous"/>
        <dc:description>Magnesium is a chemical element with the symbol Mg and atomic number 12. Elemental magnesium is a gray-white lightweight metal, two-thirds the density of aluminium. Magnesium has the lowest melting (923 K (650 C)) and the lowest boiling point 1,363 K (1,090 C) of all the alkaline earth metals</dc:description>
        <rdfs:label>Magnesium</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Complete -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Complete">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/Status"/>
        <dc:description>A status has been completed.</dc:description>
        <rdfs:label>Complete</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/ComputerMouse -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/ComputerMouse">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/PointingDevice"/>
        <dc:description>A small, movable device that lets an user control a range of things on a computer.</dc:description>
        <rdfs:label>Computer Mouse</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/CoolingPad -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/CoolingPad">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/CoolingSystem"/>
        <dc:description>A laptop/notebook cooler, cooling pad, cooler pad or chill mat is an accessory for laptop computers intended to reduce their operating temperature when the laptop is unable to sufficiently cool itself.</dc:description>
        <rdfs:label>Cooling Pad</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/CoolingRate -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/CoolingRate">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/FunctionalIndicator"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/CoolingSystem -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/CoolingSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/HardwareComponent"/>
