        <dc:description>A process that has been delayed before or while running.</dc:description>
        <rdfs:label>Delayed</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DeliveryTime -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DeliveryTime">
