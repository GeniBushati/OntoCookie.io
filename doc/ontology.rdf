        <rdfs:label>Audit</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/AuthenticationSoftware -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/AuthenticationSoftware">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/SecuritySoftware"/>
