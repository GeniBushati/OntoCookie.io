    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Data">
        <dc:description>Data that is used for or is the result of a specific process.</dc:description>
        <rdfs:label>Data</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DataProcessing -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DataProcessing">
