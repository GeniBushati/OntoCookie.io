        <rdfs:label>Device Speed</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DigitalProductPassport -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DigitalProductPassport">
        <dc:description>A collection of static and dynamic data associated with an ICT device that can be interpreted by machines and humans in order to support sustainable decision making.</dc:description>
        <rdfs:label>Digital Product Passport</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Display -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Display">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/HardwareComponent"/>
        <dc:description>A computer&apos;s display.</dc:description>
        <rdfs:label>Display</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/EconomicalIndicator -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/EconomicalIndicator">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/Indicator"/>
        <dc:description>Economic indicators allow analysis of economic performance.</dc:description>
        <rdfs:label>Economical Indicator</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/ElectricalSwict -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/ElectricalSwict">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/RePlanIT/Switch"/>
