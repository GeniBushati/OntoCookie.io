        <rdfs:label>Delivery Time</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/Dell -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/Dell">
        <rdfs:subClassOf rdf:resource="https://schema.org/Brand"/>
        <rdfs:label>Dell</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DesktopComputer -->


    <owl:Class rdf:about="http://www.semanticweb.org/RePlanIT/DesktopComputer">
        <rdfs:subClassOf rdf:resource="http://elite.polito.it/ontologies/dogont.owl#Computer"/>
        <dc:description>A desktop computer is a personal computing device designed to fit on top of a typical office desk. It houses the physical hardware that makes a computer run and connects to input devices such as the monitor, keyboard and mouse users interact with.</dc:description>
        <rdfs:comment>Also reffered to as Personal Computer (PC).</rdfs:comment>
        <rdfs:label>Desktop Computer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/RePlanIT/DeviceOwner -->


